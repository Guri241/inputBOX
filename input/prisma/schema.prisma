
datasource minutes {
  provider = "postgresql"
  url      = "postgresql://postgres:admin1234@localhost:5432/minutes" 
}



generator client {
  provider = "prisma-client-js"
}

model mydata {
  id         Int      @id @default(autoincrement())
  name       String
  option2    String
  option3    String
  checkboxes String?
  extrainput String
  contents   String
  data       DateTime @default(now())
}